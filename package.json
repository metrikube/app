  "main": "dist/apps/api/main.js",
  "scripts": {
    "build": "rimraf dist && npm run cache:clear && nx build",
    "build:all": "rimraf dist && npm run cache:clear && nx run-many --target=build",
    "build:api": "rimraf dist && npm run cache:clear && nx build api",
    "cache:clear": "nx clear-cache",
    "format": "lint-staged",
    "lint": "nx lint",
    "prepare": "husky install",
    "start": "node dist/apps/api/main.js",
    "start:all": "NODE_ENV=local && npm run cache:clear && nx run-many --target=serve --skip-nx-cache",
    "start:api": "NODE_ENV=local && npm run cache:clear && nx serve api --skip-nx-cache",
    "start:client": "nx run web:serve --skip-nx-cache",
    "test": "nx test",
    "test:all": "nx run-many --target=test"
  },
  "private": true,
