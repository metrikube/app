{
    "name": "metrikube",
    "version": "0.7.1",
    "license": "MIT",
    "main": "dist/apps/api/main.js",
    "scripts": {
        "build": "rimraf dist && npm run cache:clear && nx build",
        "build:all": "rimraf dist && nx run-many --target=build",
        "build:all:production": "npm run build:all -- --configuration=production && npm run build:migrations",
        "build:api": "rimraf dist && npm run cache:clear && nx build api",
        "build:migrations": "rimraf dist-migrations && tsc --p apps/api/tsconfig.app.json && tsc-alias -p apps/api/tsconfig.app.json",
        "cache:clear": "nx clear-cache",
        "db:init": "npm run migration:run",
        "db:init:production": "npm run migration:run:production",
        "db:revert": "npm run migration:revert",
        "db:revert:production": "npm run migration:revert:production",
        "format": "lint-staged",
        "lint": "nx lint",
        "start": "node dist/apps/api/main.js",
        "start:all": "NODE_ENV=local && npm run cache:clear && nx run-many --target=serve --skip-nx-cache",
        "start:api": "NODE_ENV=local && npm run cache:clear && nx serve api --skip-nx-cache",
        "start:client": "nx run web:serve --skip-nx-cache",
        "test": "nx test",
        "test:all": "nx run-many --target=test",
        "typeorm": "ts-node --project apps/api/tsconfig.app.json ./node_modules/typeorm/cli",
        "migration:create": "npm run typeorm migration:create ./data/migrations/$npm_config_name",
        "migration:generate": "npm run typeorm migration:generate ./data/migrations/$npm_config_name -- -d ./data/typeorm.config.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d data/typeorm.config.ts",
        "migration:run": "rimraf dist-migrations && npm run typeorm migration:run -- -d data/typeorm.config.ts",
        "migration:show": "npm run typeorm migration:show -- -d data/typeorm.config.ts",
        "schema:sync": "npm run typeorm schema:sync -- -d data/typeorm.config.ts",
        "typeorm:production": "ts-node ./node_modules/typeorm/cli",
        "migration:create:production": "npm run typeorm:production migration:create ./data/migrations/$npm_config_name",
        "migration:generate:production": "npm run typeorm:production migration:generate ./data/migrations/$npm_config_name -- -d ./data/typeorm.config.ts",
        "migration:revert:production": "npm run typeorm:production migration:revert -- -d dist-migrations/data/typeorm.config.js",
        "migration:run:production": "npm run typeorm:production migration:run -- -d dist-migrations/data/typeorm.config.js",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@aws-sdk/client-cost-explorer": "^3.357.0",
        "@aws-sdk/client-ec2": "^3.357.0",
        "@aws-sdk/client-s3": "^3.405.0",
        "@aws-sdk/credential-providers": "^3.357.0",
        "@aws-sdk/types": "^3.357.0",
        "@aws-sdk/util-credentials": "^3.56.0",
        "@emotion/css": "^11.11.2",
        "@fastify/static": "^6.10.1",
        "@mui/base": "^5.0.0-beta.11",
        "@mui/icons-material": "^5.11.16",
        "@mui/lab": "^5.0.0-alpha.135",
        "@nestjs/common": "9.4.2",
        "@nestjs/config": "^2.3.2",
        "@nestjs/core": "9.4.2",
        "@nestjs/platform-express": "^9.4.3",
        "@nestjs/platform-fastify": "^9.4.2",
        "@nestjs/schedule": "^3.0.1",
        "@nestjs/serve-static": "^4.0.0",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/typeorm": "^9.0.1",
        "@swc/helpers": "~0.5.0",
        "@tanstack/react-query": "^4.29.7",
        "axios": "^1.4.0",
        "better-sqlite3": "^8.4.0",
        "chart.js": "^4.4.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dayjs": "^1.11.9",
        "dotenv": "^16.0.3",
        "fastify": "^4.7.0",
        "masonry-layout": "^4.2.2",
        "mysql2": "^3.5.1",
        "nodemailer": "^6.9.3",
        "react": "18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.45.1",
        "react-syntax-highlighter": "^15.5.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^5.0.1",
        "rxjs": "^7.8.1",
        "sql-formatter": "^13.0.0",
        "sqlite3": "^5.1.6",
        "tslib": "^2.5.2",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.5.0",
        "@nestjs/schematics": "9.2.0",
        "@nestjs/testing": "9.4.2",
        "@nrwl/webpack": "16.2.1",
        "@nx/cypress": "16.2.2",
        "@nx/eslint-plugin": "16.2.1",
        "@nx/jest": "16.2.1",
        "@nx/js": "^16.2.2",
        "@nx/linter": "16.2.1",
        "@nx/nest": "16.2.1",
        "@nx/node": "16.2.1",
        "@nx/react": "^16.2.2",
        "@nx/vite": "16.2.2",
        "@nx/workspace": "16.2.1",
        "@playwright/test": "^1.37.1",
        "@swc/cli": "~0.1.62",
        "@swc/core": "~1.3.51",
        "@testing-library/react": "14.0.0",
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/cron": "^2.0.1",
        "@types/jest": "^29.5.1",
        "@types/masonry-layout": "^4.2.5",
        "@types/node": "20.2.3",
        "@types/nodemailer": "^6.4.8",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/react-syntax-highlighter": "^15.5.7",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "@vitejs/plugin-react": "^3.0.0",
        "@vitest/coverage-c8": "^0.31.0",
        "@vitest/ui": "^0.31.0",
        "cross-env": "^7.0.3",
        "cypress": "^12.11.0",
        "eslint": "~8.41.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "jest-environment-node": "^29.5.0",
        "jsdom": "~20.0.3",
        "lint-staged": "^13.2.2",
        "nx": "16.2.1",
        "prettier": "^2.8.8",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "tsc-alias": "^1.8.7",
        "typescript": "5.0.4",
        "vite": "^4.3.4",
        "vite-plugin-dts": "~1.7.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-tsconfig-paths": "^4.0.2",
        "vitest": "^0.31.0"
    }
}
